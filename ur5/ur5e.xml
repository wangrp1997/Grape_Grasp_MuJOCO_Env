<mujoco model="ur5e">
  <compiler angle="radian" meshdir="assets" autolimits="true"/>
  <option integrator="implicitfast" impratio="10" gravity = "0 0 -9.81" timestep="0.002"
  cone="pyramidal" />
  <option>
<!--      <flag multiccd="enable"/>-->
    <flag contact="enable"/>
  </option>
  <extension>
    <plugin plugin="mujoco.sensor.touch_grid"/>
